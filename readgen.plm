readgen:
do;

$include (glob.inc)
$include (toklit.inc)
$include (extdcl.inc)


/* MAIN PROGRAM */
declare tail$ptr pointer;

call get_command_tail(tail$ptr);
call print_string(@('string entered: {', 0));
call print_string(tail$ptr);
call print_line(@('}', 0));
call reader$set$ptr(tail$ptr);
do while get$token <> tok$eof;
    call print$token;
    call new_line;
    call tok$next;
end;

call terminate(0);

end;
